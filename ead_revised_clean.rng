	<define name="e.head">
		<element name="head">
			<ref name="a.common"/>
			<optional>
				<attribute name="altHead"/>
			</optional>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	
	<define name="e.emph">
		<element name="emph">
			<ref name="a.common"/>
			<optional>
				<attribute name="render">
					<choice>
						<ref name="av.render"/>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="altRender"/>
			</optional>
			<text/>
		</element>
	</define>
	
	<define name="e.lb">	
		<element name="lb">
			<ref name="a.common.empty"/>
			<empty/>
		</element>
	</define>
	
	<define name="e.ref">
		<element name="ref">
			<ref name="a.common"/>
		    <text/>
		</element>
	</define>
	
	<define name="e.dsc">
		<element name="dsc">
			<ref name="a.common"/>
			<optional>
				<attribute name="dscType">
					<choice>
						<value>analyticOver</value>
						<value>combined</value>
						<value>inDepth</value>
						<value>otherType</value>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="otherType">
					<data type="NMTOKEN"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="encodingAnalog"/>
			</optional>
			<ref name="a.dsctab.tPattern"/>
			<group>
				<optional>
					<ref name="e.head"/>
				</optional>
				<zeroOrMore>
					<ref name="m.blocks"/>
				</zeroOrMore>
			</group>
			<choice>

				<ref name="m.cOrC01"/>

			</choice>
		</element>
	</define>
	
	<define name="e.div">
		<element name="div">
			<ref name="a.common"/>
			<optional>
				<ref name="e.head"/>
			</optional>
			<zeroOrMore>
				<ref name="m.blocks"/>
			</zeroOrMore>
			<zeroOrMore>
				<ref name="e.div"/>
			</zeroOrMore>
		</element>
	</define>
	
	<define name="e.bibRef">
		<element name="bibRef">
			<ref name="a.common"/>
			<optional>
				<attribute name="encodingAnalog"/>
			</optional>
			<zeroOrMore>
				<choice>
					<ref name="m.mixed.basic"/>
					<ref name="e.title"/>
				</choice>
			</zeroOrMore>
		</element>
	</define>
	
	<define name="e.archRef">
		<element name="archRef">
			<ref name="a.common"/>
			<optional>
				<attribute name="encodingAnalog"/>
			</optional>
			<zeroOrMore>
				<choice>
					<text/>
					<ref name="m.mixed.basic"/>
					<ref name="e.title"/>
				</choice>
			</zeroOrMore>
		</element>
	</define>
	
	<define name="e.title">
		<element name="title">
			<ref name="a.common"/>
			<optional>
				<attribute name="localType"/>
			</optional>
			<optional>
				<attribute name="render">
					<choice>
						<ref name="av.render"/>
					</choice>
				</attribute>
			</optional>
			<ref name="a.access"/>
			<optional>
				<attribute name="encodingAnalog"/>
			</optional>
			<zeroOrMore>
             <ref name="m.mixed.basic"/>
			</zeroOrMore>
		</element>
	</define>
	
    <!-- ATTRIBUTE VALUE LISTS -->
    
	<define name="av.level">
		<choice>
			<value>class</value>
			<value>collection</value>
			<value>file</value>
			<value>fonds</value>
			<value>item</value>
			<value>otherLevel</value>
			<value>recordGrp</value>
			<value>series</value>
			<value>subFonds</value>
			<value>subGrp</value>
			<value>subSeries</value>
		</choice>
	</define>
	
	<define name="av.render">
		<choice>
			<value>altRender</value>
			<value>bold</value>
			<value>boldDoubleQuote</value>
			<value>boldItalic</value>
			<value>boldSingleQuote</value>
			<value>boldSmCaps</value>
			<value>boldUnderline</value>
			<value>doubleQuote</value>
			<value>italic</value>
			<value>nonProPort</value>
			<value>singleQuote</value>
			<value>smCaps</value>
			<value>sub</value>
			<value>super</value>
			<value>underline</value>
		</choice>
	</define>
    
    <!-- MODELS -->
	<define name="a.dsctab.tPattern">
		<optional>
			<attribute name="tPattern">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
	</define>
	<define name="a.common.empty">
		<optional>
			<attribute name="id">
				<data type="ID"/>
			</attribute>
		</optional>
		<optional>
			<attribute name="altRender"/>
		</optional>
		<optional>
			<attribute name="audience">
				<choice>
					<value>external</value>
					<value>internal</value>
				</choice>
			</attribute>
		</optional>
		<optional>
			<attribute name="altRender"/>
		</optional>
	</define>
	<define name="a.common">
		<ref name="a.common"/>
		<optional>
			<attribute name="lang"/>
			<attribute name="script"/>
		</optional>
	</define>
	
	<define name="a.desc.base">
		<optional>
			<attribute name="otherLevel">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
		<optional>
			<attribute name="encodingAnalog"/>
		</optional>
	</define>	
	<define name="a.access.no.normal">
		<optional>
			<attribute name="source">
				<data type="token"/>
			</attribute>
		</optional>
		<optional>
			<attribute name="rules">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
		<optional>
			<attribute name="identifier"/>
		</optional>
	</define>
	
	<define name="a.access">
		<ref name="a.access.no.normal"/>
		<optional>
			<attribute name="normal"/>
		</optional>
	</define>
	
	<define name="m.desc.base">
		<choice>
			<ref name="e.accessRestrict"/>
			<ref name="e.accruals"/>
			<ref name="e.acqInfo"/>
			<ref name="e.altFormAvail"/>
			<ref name="e.appraisal"/>
			<ref name="e.arrangement"/>
			<ref name="e.bibliography"/>
			<ref name="e.biogHist"/>
			<ref name="e.controlAccess"/>
			<ref name="e.custodHist"/>
			<ref name="e.index"/>
			<ref name="e.legalStatus"/>
			<ref name="e.materialSpec"/>
			<ref name="e.odd"/>
			<ref name="e.originalsLoc"/>
			<ref name="e.otherFindAid"/>
			<ref name="e.physTech"/>
			<ref name="e.preferCite"/>
			<ref name="e.processInfo"/>
			<ref name="e.relatedMaterial"/>
			<ref name="e.scopeContent"/>
			<ref name="e.separatedMaterial"/>
			<ref name="e.useRestrict"/>
		</choice>
	</define>
	<define name="m.did">
		<choice>
			<element name="abstract">
				<ref name="a.common"/>
				<optional>
					<attribute name="label"/>
				</optional>
				<optional>
					<attribute name="encodingAnalog"/>
				</optional>
				<optional>
					<attribute name="localType"/>
				</optional>
				<ref name="am.langCode"/>
				<ref name="m.mixed.basic"/>
			</element>
			<element name="langMaterial">
				<ref name="a.common"/>
				<optional>
					<attribute name="label"/>
				</optional>
				<optional>
					<attribute name="encodingAnalog"/>
				</optional>
				<ref name="m.mixed.basic"/>
			</element>
			<element name="didNote">
				<ref name="a.common"/>
				<optional>
					<attribute name="label"/>
				</optional>
				<optional>
					<attribute name="encodingAnalog"/>
				</optional>
				<optional>
					<attribute name="localType"/>
				</optional>
				<optional>
					<attribute name="localTypeSource"/>
				</optional>
				<ref name="am.langCode"/>
				<ref name="m.mixed.basic"/>
			</element>

			<ref name="e.origination"/>


			<element name="physDesc">
				<ref name="a.common"/>
				<optional>
					<attribute name="label"/>
				</optional>
				<optional>
					<attribute name="encodingAnalog"/>
				</optional>
				<optional>
					<attribute name="source">
						<data type="token"/>
					</attribute>
				</optional>
				<optional>
					<attribute name="identifier"/>
				</optional>
				<optional>
					<attribute name="rules">
						<data type="NMTOKEN"/>
					</attribute>
				</optional>
				<ref name="m.mixed.basic"/>
			</element>

			<element name="physDescStructured">
				<ref name="a.common"/>
				<optional>
					<attribute name="label"/>
				</optional>
				<optional>
					<attribute name="encodingAnalog"/>
				</optional>
				<optional>
					<attribute name="source">
						<data type="token"/>
					</attribute>
				</optional>
				<optional>
					<attribute name="identifier"/>
				</optional>
				<optional>
					<attribute name="rules">
						<data type="NMTOKEN"/>
					</attribute>
				</optional>
				<optional>
					<attribute name="physDescStructuredType">
						<choice>
							<value>carrier</value>
							<value>materialType</value>
							<value>spaceOccupied</value>
						</choice>
					</attribute>
				</optional>
				<zeroOrMore>
					<choice>
						<element name="specifiedMaterials">
							<ref name="a.common"/>
							<optional>
								<attribute name="label"/>
							</optional>
							<optional>
								<attribute name="localType"/>
							</optional>
							<optional>
								<attribute name="localTypeSource"/>
							</optional>
							<optional>
								<attribute name="encodinganalog"/>
							</optional>
							<ref name="m.mixed.basic"/>
						</element>
						<element name="physDetails">
							<ref name="a.common"/>
							<optional>
								<attribute name="label"/>
							</optional>
							<optional>
								<attribute name="localType"/>
							</optional>
							<optional>
								<attribute name="localTypeSource"/>
							</optional>
