<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ead3="http://ead3.archivists.org/schema/" elementFormDefault="qualified" targetNamespace="http://ead3.archivists.org/schema/">
   <xs:element name="ead">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="control" type="ead3:control"/>
            <xs:element name="archdesc" type="ead3:archdesc"/>
         </xs:sequence>
         <xs:attributeGroup ref="ead3:a.common"/>
         <xs:attribute name="relatedencoding"/>
      </xs:complexType>
   </xs:element>
   <xs:complexType name="archdesc">
      <xs:sequence>
         <xs:element name="did" type="ead3:did"/>
         <xs:element name="m.desc.base.full" type="ead3:m.desc.base.full" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attribute name="level" use="required" type="ead3:av.level"/>
      <xs:attributeGroup ref="ead3:a.desc.base"/>
      <xs:attribute name="relatedencoding"/>
   </xs:complexType>
   <xs:complexType name="control">
      <xs:sequence>
         <xs:element name="recordid" type="ead3:recordid"/>
         <xs:element name="otherrecordid" type="ead3:otherrecordid" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="representation" type="ead3:representation" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="filedesc" type="ead3:filedesc"/>
         <xs:element name="maintenancestatus" type="ead3:maintenancestatus"/>
         <xs:element name="publicationstatus" type="ead3:publicationstatus" minOccurs="0"/>
         <xs:element name="maintenanceagency" type="ead3:maintenanceagency"/>
         <xs:element name="languagedeclaration" type="ead3:languagedeclaration" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="conventiondeclaration" type="ead3:conventiondeclaration" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="localtypedeclaration" type="ead3:localtypedeclaration" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="localcontrol" type="ead3:localcontrol" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="maintenancehistory" type="ead3:maintenancehistory"/>
         <xs:element name="sources" type="ead3:sources" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attribute name="base" type="xs:anyURI"/>
      <xs:attribute name="langencoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso639-2b"/>
               <xs:enumeration value="other"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="scriptencoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso15924"/>
               <xs:enumeration value="other"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="dateencoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso8601"/>
               <xs:enumeration value="other"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="countryencoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso3166-1"/>
               <xs:enumeration value="other"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="repositoryencoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso15511"/>
               <xs:enumeration value="other"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="relatedencoding"/>
   </xs:complexType>
   <xs:complexType name="recordid" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="instanceurl" type="xs:anyURI"/>
   </xs:complexType>
   <xs:complexType name="otherrecordid" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
   </xs:complexType>
   <xs:complexType name="representation" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:am.simplelink"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
   </xs:complexType>
   <xs:complexType name="filedesc">
      <xs:sequence>
         <xs:element name="titlestmt" type="ead3:titlestmt"/>
         <xs:element name="editionstmt" type="ead3:editionstmt" minOccurs="0"/>
         <xs:element name="publicationstmt" type="ead3:publicationstmt" minOccurs="0"/>
         <xs:element name="seriesstmt" type="ead3:seriesstmt" minOccurs="0"/>
         <xs:element name="notestmt" type="ead3:notestmt" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="titlestmt">
      <xs:sequence>
         <xs:element name="titleproper" type="ead3:titleproper" maxOccurs="unbounded"/>
         <xs:element name="subtitle" type="ead3:subtitle" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="author" type="ead3:author" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="sponsor" type="ead3:sponsor" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="editionstmt">
      <xs:choice maxOccurs="unbounded">
         <xs:element name="edition" type="ead3:edition"/>
         <xs:element name="p" type="ead3:p"/>
      </xs:choice>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="publicationstmt">
      <xs:choice maxOccurs="unbounded">
         <xs:element name="publisher" type="ead3:publisher"/>
         <xs:element name="date" type="ead3:date"/>
         <xs:element name="address" type="ead3:address"/>
         <xs:element name="num" type="ead3:num"/>
         <xs:element name="p" type="ead3:p"/>
      </xs:choice>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="seriesstmt">
      <xs:choice maxOccurs="unbounded">
         <xs:element name="titleproper" type="ead3:titleproper"/>
         <xs:element name="num" type="ead3:num"/>
         <xs:element name="p" type="ead3:p"/>
      </xs:choice>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="notestmt">
      <xs:sequence>
         <xs:element name="controlnote" type="ead3:controlnote" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="maintenancestatus" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="value" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="revised"/>
               <xs:enumeration value="deleted"/>
               <xs:enumeration value="new"/>
               <xs:enumeration value="deletedsplit"/>
               <xs:enumeration value="deletedmerged"/>
               <xs:enumeration value="deletedreplaced"/>
               <xs:enumeration value="cancelled"/>
               <xs:enumeration value="derived"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="publicationstatus" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="value" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="inprocess"/>
               <xs:enumeration value="approved"/>
               <xs:enumeration value="published"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="languagedeclaration">
      <xs:sequence>
         <xs:element name="language" type="ead3:language"/>
         <xs:element name="script" type="ead3:script"/>
         <xs:element name="descriptivenote" type="ead3:descriptivenote" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="conventiondeclaration">
      <xs:sequence>
         <xs:element name="abbr" type="ead3:abbr" minOccurs="0"/>
         <xs:element name="citation" type="ead3:citation"/>
         <xs:element name="descriptivenote" type="ead3:descriptivenote" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="localtypedeclaration">
      <xs:sequence>
         <xs:element name="abbr" type="ead3:abbr" minOccurs="0"/>
         <xs:element name="citation" type="ead3:citation"/>
         <xs:element name="descriptivenote" type="ead3:descriptivenote" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="localcontrol">
      <xs:sequence>
         <xs:element name="term" type="ead3:term" minOccurs="0"/>
         <xs:choice minOccurs="0">
            <xs:element name="datesingle" type="ead3:datesingle"/>
            <xs:element name="daterange" type="ead3:daterange"/>
         </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
   </xs:complexType>
   <xs:complexType name="term" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="transliteration"/>
      <xs:attribute name="lastdatetimeverified" type="xs:token"/>
      <xs:attributeGroup ref="ead3:a.access.no.normal"/>
   </xs:complexType>
   <xs:complexType name="maintenancehistory">
      <xs:sequence>
         <xs:element name="maintenanceevent" type="ead3:maintenanceevent" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="maintenanceevent">
      <xs:sequence>
         <xs:element name="eventtype" type="ead3:eventtype"/>
         <xs:element name="eventdatetime" type="ead3:eventdatetime"/>
         <xs:element name="agenttype" type="ead3:agenttype"/>
         <xs:element name="agent" type="ead3:agent"/>
         <xs:element name="eventdescription" type="ead3:eventdescription" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="eventtype" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="value" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="created"/>
               <xs:enumeration value="revised"/>
               <xs:enumeration value="deleted"/>
               <xs:enumeration value="cancelled"/>
               <xs:enumeration value="derived"/>
               <xs:enumeration value="updated"/>
               <xs:enumeration value="unknown"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="eventdatetime" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="standarddatetime" type="xs:token"/>
   </xs:complexType>
   <xs:complexType name="agenttype" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="value" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="human"/>
               <xs:enumeration value="machine"/>
               <xs:enumeration value="unknown"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="agent" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="eventdescription" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
   </xs:complexType>
   <xs:complexType name="sources">
      <xs:sequence>
         <xs:element name="source" type="ead3:source" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attribute name="base" type="xs:anyURI"/>
   </xs:complexType>
   <xs:complexType name="source">
      <xs:sequence>
         <xs:element name="sourceentry" type="ead3:sourceentry" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="objectxmlwrap" type="ead3:objectxmlwrap" minOccurs="0"/>
         <xs:element name="descriptivenote" type="ead3:descriptivenote" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="lastdatetimeverified" type="xs:token"/>
      <xs:attributeGroup ref="ead3:am.simplelink"/>
   </xs:complexType>
   <xs:complexType name="sourceentry" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="transliteration"/>
   </xs:complexType>
   <xs:complexType name="titleproper" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attribute name="render" type="ead3:av.render"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="subtitle" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="author" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="sponsor" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="edition" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="date" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:am.dates.era"/>
      <xs:attributeGroup ref="ead3:am.dates.calendar"/>
      <xs:attributeGroup ref="ead3:am.date.normal"/>
      <xs:attribute name="certainty"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="publisher" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="controlnote">
      <xs:sequence>
         <xs:element name="m.blocks" type="ead3:m.blocks" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="maintenanceagency">
      <xs:sequence>
         <xs:element name="agencycode" type="ead3:agencycode" minOccurs="0"/>
         <xs:element name="otheragencycode" type="ead3:otheragencycode" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="agencyname" type="ead3:agencyname" maxOccurs="unbounded"/>
         <xs:element name="descriptivenote" type="ead3:descriptivenote" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:am.countrycode"/>
   </xs:complexType>
   <xs:complexType name="agencycode" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
   </xs:complexType>
   <xs:complexType name="otheragencycode" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
   </xs:complexType>
   <xs:complexType name="agencyname" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
   </xs:complexType>
   <xs:complexType name="citation" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:am.simplelink"/>
      <xs:attribute name="lastdatetimeverified" type="xs:token"/>
   </xs:complexType>
   <xs:complexType name="did">
      <xs:sequence>
         <xs:element name="head" type="ead3:head" minOccurs="0"/>
         <xs:element name="m.did" type="ead3:m.did" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="origination">
      <xs:choice maxOccurs="unbounded">
         <xs:element name="corpname" type="ead3:corpname"/>
         <xs:element name="famname" type="ead3:famname"/>
         <xs:element name="name" type="ead3:name"/>
         <xs:element name="persname" type="ead3:persname"/>
      </xs:choice>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:a.label"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="physdescstructured">
      <xs:sequence>
         <xs:element name="quantity" type="ead3:quantity"/>
         <xs:element name="unittype" type="ead3:unittype"/>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="physfacet" type="ead3:physfacet"/>
            <xs:element name="dimensions" type="ead3:dimensions"/>
         </xs:choice>
         <xs:element name="descriptivenote" type="ead3:descriptivenote" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.label"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="physdescstructuredtype" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="carrier"/>
               <xs:enumeration value="materialtype"/>
               <xs:enumeration value="spaceoccupied"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="ead3:a.coverage"/>
   </xs:complexType>
   <xs:complexType name="quantity" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="approximate" type="ead3:av.yesorno"/>
   </xs:complexType>
   <xs:complexType name="unittype" mixed="true">
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.access.no.normal"/>
   </xs:complexType>
   <xs:complexType name="repository">
      <xs:sequence>
         <xs:choice maxOccurs="unbounded">
            <xs:element name="corpname" type="ead3:corpname"/>
            <xs:element name="famname" type="ead3:famname"/>
            <xs:element name="name" type="ead3:name"/>
            <xs:element name="persname" type="ead3:persname"/>
         </xs:choice>
         <xs:element name="address" type="ead3:address" minOccurs="0"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attributeGroup ref="ead3:a.label"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="unittitle" mixed="true">
      <xs:group ref="ead3:m.mixed.basic.plus.access"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.label"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attributeGroup ref="ead3:a.localtype"/>
      <xs:attribute name="normal"/>
   </xs:complexType>
   <xs:complexType name="unitdate" mixed="true">
      <xs:group ref="ead3:m.mixed.basic"/>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.label"/>
      <xs:attribute name="unitdatetype">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="bulk"/>
               <xs:enumeration value="inclusive"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="datechar"/>
      <xs:attributeGroup ref="ead3:am.dates.era"/>
      <xs:attributeGroup ref="ead3:am.dates.calendar"/>
      <xs:attributeGroup ref="ead3:am.date.normal"/>
      <xs:attribute name="certainty"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
   </xs:complexType>
   <xs:complexType name="unitdatestructured">
      <xs:sequence>
         <xs:element name="m.dates" type="ead3:m.dates"/>
      </xs:sequence>
      <xs:attributeGroup ref="ead3:a.common"/>
      <xs:attributeGroup ref="ead3:a.label"/>
      <xs:attribute name="unitdatetype">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="bulk"/>
               <xs:enumeration value="inclusive"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="certainty"/>
      <xs:attributeGroup ref="ead3:a.encodinganalog"/>
      <xs:attribute name="datechar"/>
      <xs:attributeGroup ref="ead3:am.dates.era"/>
      <xs:attributeGroup ref="ead3:am.dates.calendar"/>
   </xs:complexType>
   <xs:complexType name="datesingle">
      <xs:complexContent>
         <xs:extension base="ead3:m.datesingle">
            <xs:attributeGroup ref="ead3:a.common"/>
