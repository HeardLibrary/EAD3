<?xml-model href="../../schemas/tagLibrary_elementDesc.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<div xmlns="http://www.tei-c.org/ns/1.0"
     xmlns:eg="http://www.tei-c.org/ns/Examples"
     xmlns:ead="http://ead3.archivists.org/schema/"
     xmlns:oai_dc="http://www.openarchives.org/OAI/2.0/oai_dc/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:xi="http://www.w3.org/2001/XInclude"
     xml:lang="eng"
     xml:id="attr-countryencoding"
     type="attributeDocumentation">
   <head>
      <att>countryencoding</att>
      <seg type="fullName">Country Encoding</seg>
   </head>
   <div type="summary">
      <p>The authoritative source or rules for values supplied in
                           <att>countrycode</att> in <gi>maintenanceagency</gi> and <gi>unitid</gi>.
                        If the value "othercountryencoding" is selected an alternate code list
                        should be specified in <gi>conventiondeclaration</gi>. Available only in
                           <gi>control</gi>.</p>
   </div>
   <div type="values">
      <p>iso3166-1, othercountryencoding</p>
   </div>
</div>
