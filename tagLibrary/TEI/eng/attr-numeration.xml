<?xml-model href="../../schemas/tagLibrary_elementDesc.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<div xmlns="http://www.tei-c.org/ns/1.0"
     xmlns:eg="http://www.tei-c.org/ns/Examples"
     xmlns:ead="http://ead3.archivists.org/schema/"
     xmlns:oai_dc="http://www.openarchives.org/OAI/2.0/oai_dc/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:xi="http://www.w3.org/2001/XInclude"
     xml:lang="eng"
     xml:id="attr-numeration"
     type="attributeDocumentation">
   <head>
      <att>numeration</att>
      <seg type="fullName">Numeration</seg>
   </head>
   <div type="summary">
      <p>For lists with a <att>listtype</att> value of "ordered,"
                           <att>numeration</att> specifies the type of numeration. </p>
   </div>
   <div type="values">
      <p>armenian, decimal, decimal-leading-zero, georgian, inherit, lower-alpha, lower-greek, lower-latin, lower-roman, upper-alpha, upper-latin, upper-roman</p>
   </div>
</div>
