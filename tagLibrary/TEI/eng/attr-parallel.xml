     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:xi="http://www.w3.org/2001/XInclude"
     xml:lang="eng"
     xml:id="attr-parallel"
     type="attributeDocumentation">
   <head>
      <att>parallel</att>
      <seg type="fullName">Parallel</seg>
   </head>
   <div type="summary">
      <p>Specifies if the statements of physical description in a
                           <gi>physdescset</gi> are parallel to one another or not (that is, they
                        are alternate descriptions of the same set of material). Optional in
                           <gi>physdescset</gi>.</p>
   </div>
   <div type="values">
      <p>part, whole</p>
   </div>
</div>
