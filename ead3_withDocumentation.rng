         <rng:define name="am.access">
            <ref name="am.access.no.normal"/>
            <optional>
               <ref name="a.normal"/>
            </optional>
         </rng:define>
         <rng:define name="am.access.no.normal">
            <optional>
               <attribute name="source">
                  <a:documentation xml:lang="eng">The controlled vocabulary that is the source of the term contained in the
                        element. Available in &lt;corpname&gt;, &lt;famname&gt;,
                        &lt;function&gt;, &lt;genreform&gt;, &lt;geogname&gt;, &lt;name&gt;,
                           &lt;occupation&gt;, &lt;part&gt;, &lt;persname&gt;,
                        &lt;physfacet&gt;, &lt;subject&gt;, &lt;term&gt;, &lt;title&gt;,
                           &lt;unitid&gt;, and &lt;unittype&gt;.A child element of &lt;sources&gt; used to identify a particular source of
                     evidence used in describing the archival material.</a:documentation>
                  <a:documentation xml:lang="eng">Use &lt;source&gt; to cite a published resource used in describing the
                     archival material. Though not required, a reference for the source should be
                     included as a textual description in the child &lt;sourceentry&gt;. Use the
                     optional &lt;descriptivenote&gt; for any additional notes about the source.
                     Use the optional &lt;objectxmlwrap&gt; to embed XML documenting the source
                     from any namespace other than EAD3. The child elements of &lt;source&gt; are optional, but when present they must
                     be provided in a specific order:&lt;sourceentry&gt;&lt;objectxmlwrap&gt;&lt;descriptivenote&gt;See also:Do not confuse with &lt;citation&gt;, used in
                           &lt;conventiondeclaration&gt; and &lt;localtypedeclaration&gt; to
                        identify any rules and conventions applied in the description.</a:documentation>
                  <data type="token"/>
               </attribute>
            </optional>
            <optional>
               <attribute name="rules">
                  <a:documentation xml:lang="eng">Name of the descriptive rules or conventions that govern the formulation of
                        the content of the element. Available in &lt;corpname&gt;,
                        &lt;famname&gt;, &lt;function&gt;, &lt;genreform&gt;, &lt;geogname&gt;,
                           &lt;name&gt;, &lt;occupation&gt;, &lt;part&gt;, &lt;persname&gt;,
                           &lt;physfacet&gt;, &lt;subject&gt;, &lt;term&gt;, &lt;title&gt;,
                           &lt;unitid&gt;, &lt;unittype&gt;.</a:documentation>
                  <data type="NMTOKEN"/>
               </attribute>
            </optional>
            <optional>
               <ref name="a.identifier"/>
            </optional>
         </rng:define>
         <!-- ATTRIBUTE DEFINITIONS -->
         <!-- xlink-modelled attributes -->
         <rng:define name="a.link.actuate">
            <attribute name="actuate">
               <a:documentation xml:lang="eng">A control that defines whether a rendering application should present an
                        actionable link automatically (onload) or when requested by the user
                        (onrequest). It is used in conjunction with @show to determine
                        link behavior. </a:documentation>
