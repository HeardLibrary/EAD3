                     attribute audience { "external" | "internal" }?,
                     attribute lang { text }?,
                     attribute script { text }?,
                     e.geogname*,
                     e.event+
                   }+)
              }+
            }
            | e.list
            | e.table)
           | e.p)+
        })
     | e.p)+
  }
e.event =
  element event {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    (((e.persname
       | e.corpname
       | e.famname
       | e.geogname
       | e.name
       | e.occupation
       | e.subject
       | e.genreform
       | e.function
       | e.title)
      | (text
         | e.abbr
         | e.expan
         | e.emph
         | e.lb
         | e.ref
         | e.ptr
         | e.foreign)*
      | e.quote
      | e.num
      | e.footnote
      | e.date)*
     | e.list)*
  }
# relations
e.relations =
  element relations {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute encodinganalog { text }?,
    attribute localtype { text }?,
    attribute base { xsd:anyURI }?,
    element relation {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute relationtype {
        "cpfrelation"
        | "resourcerelation"
        | "functionrelation"
        | "otherrelation"
      },
      attribute otherrelationtype { xsd:string }?,
      attribute lastdatetimeverified {
        # <ref name="m.data.standardDateTime"/>
        xsd:token
      }?,
      attribute linktype { "simple" }?,
      attribute href { xsd:anyURI }?,
      attribute linkrole { xsd:anyURI }?,
      attribute arcrole { xsd:anyURI }?,
      attribute linktitle { text }?,
      attribute show {
        "new" | "replace" | "embed" | "other" | "none"
      }?,
      attribute actuate { "onLoad" | "onRequest" | "other" | "none" }?,
      element relationentry {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        attribute localtype { text }?,
        attribute transliteration { xsd:NMTOKEN }?,
        text
      }*,
      e.objectxmlwrap?,
      (e.datesingle | e.daterange | e.dateset)?,
      e.geogname?,
      e.descriptivenote?
    }+
  }
e.objectxmlwrap =
  element objectxmlwrap {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    e.anyname
  }
e.anyname =
  element * - ns1:* {
    # <zeroOrMore>
    #	<attribute>
    #		<anyName/>
    #	</attribute>
    # </zeroOrMore>
    # 
    # <zeroOrMore>
    #	<choice>
    #		<ref name="e.anyname"/>
    #		<text/>
    #	</choice>
    # </zeroOrMore>
    e.anyname*
  }
