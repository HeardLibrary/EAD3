namespace a = "http://relaxng.org/ns/compatibility/annotations/1.0"
default namespace ns1 = "http://ead3.archivists.org/schema/"

start = e.ead
e.ead =
  element ead {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute relatedencoding { text }?,
    e.control,
    element archdesc {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute level {
        "class"
        | "collection"
        | "file"
        | "fonds"
        | "item"
        | "otherlevel"
        | "recordgrp"
        | "series"
        | "subfonds"
        | "subgrp"
        | "subseries"
      },
      attribute otherlevel { xsd:NMTOKEN }?,
      attribute encodinganalog { text }?,
      attribute relatedencoding { text }?,
      e.did,
      ((e.accessrestrict
        | e.accruals
        | e.acqinfo
        | e.altformavail
        | e.appraisal
        | e.arrangement
        | e.bibliography
        | e.bioghist
        | e.controlaccess
        | e.custodhist
        | e.fileplan
        | e.index
        | e.legalstatus
        | e.odd
        | e.originalsloc
        | e.otherfindaid
        | e.phystech
        | e.prefercite
        | e.processinfo
        | e.relatedmaterial
        | e.relations
        | e.scopecontent
        | e.separatedmaterial
        | e.userestrict)
       | e.dsc)*
    }
  }
# ELEMENTS

# Control
e.control =
  element control {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute base { xsd:anyURI }?,
    attribute langencoding { '"iso639-2b' | "other" }?,
    attribute scriptencoding { "iso15924" | "other" }?,
    attribute dateencoding { "iso8601" | "other" }?,
    attribute countryencoding { "iso3166-1" | "other" }?,
    attribute repositoryencoding { "iso15511" | "other" }?,
    attribute relatedencoding { text }?,
    element recordid {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute instanceurl { xsd:anyURI }?,
      text
    },
    element otherrecordid {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute localtype { text }?,
      text
    }*,
    element representation {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute linktype { "simple" }?,
      attribute href { xsd:anyURI }?,
      attribute linkrole { xsd:anyURI }?,
      attribute arcrole { xsd:anyURI }?,
      attribute linktitle { text }?,
      attribute show {
        "new" | "replace" | "embed" | "other" | "none"
      }?,
      attribute actuate { "onLoad" | "onRequest" | "other" | "none" }?,
      attribute localtype { text }?,
      text
    }*,
    element filedesc {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      element titlestmt {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        e.titleproper+,
        e.subtitle*,
        e.author*,
        e.sponsor*
      },
      element editionstmt {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        (e.edition | e.p)+
      }?,
      element publicationstmt {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        (e.publisher | e.date | e.address | e.num | e.p)+
      }?,
      element seriesstmt {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        (e.titleproper | e.num | e.p)+
      }?,
      element notestmt {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        e.controlnote+
      }?
    },
    element maintenancestatus {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute value {
        "revised"
        | "deleted"
        | "new"
        | "deletedsplit"
        | "deletedmerged"
        | "deletedreplaced"
        | "cancelled"
        | "derived"
      },
      text
    },
    element publicationstatus {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute value { "inprocess" | "approved" | "published" },
      text
    }?,
    e.maintenanceagency,
    element languagedeclaration {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      e.language,
      e.script,
      e.descriptivenote?
    }*,
    element conventiondeclaration {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      e.abbr?,
      e.citation,
      e.descriptivenote?
    }*,
    element localtypedeclaration {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      e.abbr?,
      e.citation,
      e.descriptivenote?
    }*,
    element localcontrol {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute localtype { text }?,
      element term {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        attribute transliteration { text }?,
        attribute lastdatetimeverified {
          # <ref name="m.data.standardDateTime"/>
          xsd:token
        }?,
        attribute source { xsd:token }?,
        attribute rules { xsd:NMTOKEN }?,
        attribute identifier { text }?,
        text
      }?,
      (e.datesingle | e.daterange)?
    }*,
    element maintenancehistory {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      element maintenanceevent {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        element eventtype {
          attribute id { xsd:ID }?,
          attribute altrender { text }?,
          attribute audience { "external" | "internal" }?,
          attribute lang { text }?,
          attribute script { text }?,
          attribute encodinganalog { text }?,
          attribute value {
            "created"
            | "revised"
            | "deleted"
            | "cancelled"
            | "derived"
            | "updated"
            | "unknown"
          },
          text
        },
        element eventdatetime {
          attribute id { xsd:ID }?,
          attribute altrender { text }?,
          attribute audience { "external" | "internal" }?,
          attribute lang { text }?,
          attribute script { text }?,
          attribute encodinganalog { text }?,
          attribute standarddatetime {
            # <ref name="m.data.standardDateTime"/>
            xsd:token
          }?,
          text
        },
        element agenttype {
          attribute id { xsd:ID }?,
          attribute altrender { text }?,
          attribute audience { "external" | "internal" }?,
          attribute lang { text }?,
          attribute script { text }?,
          attribute encodinganalog { text }?,
          attribute value { "human" | "machine" | "unknown" },
          text
        },
        [ a:id = "agent" ]
        element agent {
          attribute id { xsd:ID }?,
          attribute altrender { text }?,
          attribute audience { "external" | "internal" }?,
          attribute lang { text }?,
          attribute script { text }?,
          attribute encodinganalog { text }?,
          text
        },
        [ a:id = "eventdesccription" ]
        element eventdescription {
          attribute id { xsd:ID }?,
          attribute altrender { text }?,
          attribute audience { "external" | "internal" }?,
          attribute lang { text }?,
          attribute script { text }?,
          attribute encodinganalog { text }?,
          attribute localtype { text }?,
          text
        }*
      }+
    },
    element sources {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute localtype { text }?,
      attribute base { xsd:anyURI }?,
      element source {
        attribute id { xsd:ID }?,
        attribute altrender { text }?,
        attribute audience { "external" | "internal" }?,
        attribute lang { text }?,
        attribute script { text }?,
        attribute encodinganalog { text }?,
        attribute lastdatetimeverified {
          # <ref name="m.data.standardDateTime"/>
          xsd:token
        }?,
        attribute linktype { "simple" }?,
        attribute href { xsd:anyURI }?,
        attribute linkrole { xsd:anyURI }?,
        attribute arcrole { xsd:anyURI }?,
        attribute linktitle { text }?,
        attribute show {
          "new" | "replace" | "embed" | "other" | "none"
        }?,
        attribute actuate {
          "onLoad" | "onRequest" | "other" | "none"
        }?,
        element sourceentry {
          attribute id { xsd:ID }?,
          attribute altrender { text }?,
          attribute audience { "external" | "internal" }?,
          attribute lang { text }?,
          attribute script { text }?,
          attribute encodinganalog { text }?,
          attribute transliteration { text }?,
          text
        }*,
        e.objectxmlwrap?,
        e.descriptivenote?
      }+
    }?
  }
e.titleproper =
  element titleproper {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    attribute render {
      "altrender"
      | "bold"
      | "bolddoublequote"
      | "bolditalic"
      | "boldsinglequote"
      | "boldsmcaps"
      | "boldunderline"
      | "doublequote"
      | "italic"
      | "nonproport"
      | "singlequote"
      | "smcaps"
      | "sub"
      | "super"
      | "underline"
    }?,
    attribute encodinganalog { text }?,
    (text
     | e.abbr
     | e.expan
     | e.emph
     | e.lb
     | e.ref
     | e.ptr
     | e.foreign)*
  }
e.subtitle =
  element subtitle {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    attribute encodinganalog { text }?,
    (text
     | e.abbr
     | e.expan
     | e.emph
     | e.lb
     | e.ref
     | e.ptr
     | e.foreign)*
  }
e.author =
  element author {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    attribute encodinganalog { text }?,
    (text
     | e.abbr
     | e.expan
     | e.emph
     | e.lb
     | e.ref
     | e.ptr
     | e.foreign)*
  }
e.sponsor =
  element sponsor {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    attribute encodinganalog { text }?,
    (text
     | e.abbr
     | e.expan
     | e.emph
     | e.lb
     | e.ref
     | e.ptr
     | e.foreign)*
  }
e.edition =
  element edition {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    attribute encodinganalog { text }?,
    (text
     | e.abbr
     | e.expan
     | e.emph
     | e.lb
     | e.ref
     | e.ptr
     | e.foreign)*
  }
e.date =
  element date {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    attribute era { xsd:NMTOKEN }?,
    attribute calendar { xsd:NMTOKEN }?,
    attribute normal { xsd:token }?,
    attribute certainty { text }?,
    attribute encodinganalog { text }?,
    (text
     | e.abbr
     | e.expan
     | e.emph
     | e.lb
     | e.ref
     | e.ptr
     | e.foreign)*
  }
e.publisher =
  element publisher {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    attribute encodinganalog { text }?,
    (text
     | e.abbr
     | e.expan
     | e.emph
     | e.lb
     | e.ref
     | e.ptr
     | e.foreign)*
  }
e.controlnote =
  element controlnote {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute localtype { text }?,
    attribute encodinganalog { text }?,
    (((element chronlist {
         attribute id { xsd:ID }?,
         attribute altrender { text }?,
         attribute audience { "external" | "internal" }?,
         attribute lang { text }?,
         attribute script { text }?,
         attribute localtype { text }?,
         attribute encodinganalog { text }?,
         e.head?,
         e.listhead?,
         element chronitem {
           attribute id { xsd:ID }?,
           attribute altrender { text }?,
           attribute audience { "external" | "internal" }?,
           attribute lang { text }?,
           attribute script { text }?,
           attribute localtype { text }?,
           (e.datesingle | e.daterange | e.dateset),
           ((e.geogname?, e.event)
            | element chronitemset {
                attribute id { xsd:ID }?,
                attribute altrender { text }?,
                attribute audience { "external" | "internal" }?,
                attribute lang { text }?,
                attribute script { text }?,
                e.geogname*,
                e.event+
              }+)
         }+
       }
       | e.list
       | e.table)
      | element blockquote {
          attribute id { xsd:ID }?,
          attribute altrender { text }?,
          attribute audience { "external" | "internal" }?,
          attribute lang { text }?,
          attribute script { text }?,
          ((element chronlist {
              attribute id { xsd:ID }?,
              attribute altrender { text }?,
              attribute audience { "external" | "internal" }?,
              attribute lang { text }?,
              attribute script { text }?,
              attribute localtype { text }?,
              attribute encodinganalog { text }?,
              e.head?,
              e.listhead?,
              element chronitem {
                attribute id { xsd:ID }?,
                attribute altrender { text }?,
                attribute audience { "external" | "internal" }?,
                attribute lang { text }?,
                attribute script { text }?,
                attribute localtype { text }?,
                (e.datesingle | e.daterange | e.dateset),
                ((e.geogname?, e.event)
                 | element chronitemset {
                     attribute id { xsd:ID }?,
                     attribute altrender { text }?,
                     attribute audience { "external" | "internal" }?,
                     attribute lang { text }?,
                     attribute script { text }?,
                     e.geogname*,
                     e.event+
                   }+)
              }+
            }
            | e.list
            | e.table)
           | e.p)+
        })
     | e.p)+
  }
e.maintenanceagency =
  element maintenanceagency {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute encodinganalog { text }?,
    attribute countrycode { xsd:NMTOKEN }?,
    element agencycode {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute localtype { text }?,
      text
    }?,
    element otheragencycode {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute localtype { text }?,
      text
    }*,
    element agencyname {
      attribute id { xsd:ID }?,
      attribute altrender { text }?,
      attribute audience { "external" | "internal" }?,
      attribute lang { text }?,
      attribute script { text }?,
      attribute encodinganalog { text }?,
      attribute localtype { text }?,
      text
    }+,
    e.descriptivenote?
  }
e.citation =
  [ a:id = "citation" ]
  element citation {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute encodinganalog { text }?,
    attribute linktype { "simple" }?,
    attribute href { xsd:anyURI }?,
    attribute linkrole { xsd:anyURI }?,
    attribute arcrole { xsd:anyURI }?,
    attribute linktitle { text }?,
    attribute show { "new" | "replace" | "embed" | "other" | "none" }?,
    attribute actuate { "onLoad" | "onRequest" | "other" | "none" }?,
    attribute lastdatetimeverified {
      # <ref name="m.data.standardDateTime"/>
      xsd:token
    }?,
    (text
     | e.abbr
     | e.expan
     | e.emph
     | e.lb
     | e.ref
     | e.ptr
     | e.foreign)*
  }
# did
e.did =
  element did {
    attribute id { xsd:ID }?,
    attribute altrender { text }?,
    attribute audience { "external" | "internal" }?,
    attribute lang { text }?,
    attribute script { text }?,
    attribute encodinganalog { text }?,
    e.head?,
    (element abstract {
       attribute id { xsd:ID }?,
       attribute altrender { text }?,
       attribute audience { "external" | "internal" }?,
       attribute lang { text }?,
       attribute script { text }?,
       attribute localtype { text }?,
       attribute label { text }?,
       attribute encodinganalog { text }?,
       ((e.persname
         | e.corpname
         | e.famname
