      <xs:sequence>
        <xs:element minOccurs="0" ref="abbr"/>
        <xs:element ref="citation"/>
        <xs:element minOccurs="0" ref="descriptivenote"/>
      </xs:sequence>
      <xs:attributeGroup ref="a.common"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="localtypedeclaration">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="abbr"/>
        <xs:element ref="citation"/>
        <xs:element minOccurs="0" ref="descriptivenote"/>
      </xs:sequence>
      <xs:attributeGroup ref="a.common"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="localcontrol">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="term"/>
        <xs:choice minOccurs="0">
          <xs:element ref="datesingle"/>
          <xs:element ref="daterange"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="a.common"/>
      <xs:attribute name="localtype" use="required"/>
